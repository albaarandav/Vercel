{
    "builds": [
      {
        "src": "python_vercel_task/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/blog(.*)",
        "dest": "python_vercel_task/wsgi.py"
      }
    ]
  }